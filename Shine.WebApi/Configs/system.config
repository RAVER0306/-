<?xml version="1.0" encoding="utf-8"?>
<appSettings>
  <!-- ================== 1：开发系统相关配置 ================== -->
  <!-- 用户登陆缓存超时 单位：分钟(设置为0或设置错误都将默认30分钟)-->
  <add key="LoginTimeOut" value="15" />
  <!-- ================== 2：系统软件参数配置 ================== -->
  <!-- 软件名称 -->
  <add key="SoftName" value="雷雁智能路灯系统API接口" />
  <!-- 软件版本 -->
  <add key="Version" value="1.0" />
  <!-- 软件授权码 测试备用 -->
  <add key="LicenceKey" value="83eda07f-ead3-4050-bcee-4f41e57a7e16" />
  <!-- ================== 3：外部邮件参数 ================== -->
  <!-- 设置邮箱名称 -->
  <add key="MailName" value="邮件中心" />
  <!-- 设置邮箱中心服务地址 -->
  <add key="MailUserName" value="" />
  <!-- 设置邮箱中心服务密码 -->
  <add key="MailPassword" value="" />
  <!-- 设置邮箱主机 -->
  <add key="MailHost" value="" />
</appSettings>